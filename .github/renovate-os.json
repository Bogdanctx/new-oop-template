{
  "recreateWhen": "always",
  "enabledManagers": ["custom.regex"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [".github/workflows/*.yml"],
      "matchStrings": [
        "#\\s?renovate:\\s?depName=(?<depName>\\S+)\\s+runs-on:\\s*\\S+-(?<currentValue>\\S+)",
        "#\\s?renovate:\\s?depName=(?<depName>\\S+)\\s+-\\s?os:\\s*\\S+-(?<currentValue>\\S+)",
        "#\\s?renovate:\\s?depName=(?<depName>\\S+)[\\s\\S]*?\\bname:\\s*[\"'][^\"']*?\\b(?:Ubuntu|macOS|Windows)\\s+(?<currentValue>\\d+(?:\\.\\d+)?)\\b[^\"']*[\"']"
      ],
      "datasourceTemplate": "github-runners"
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
