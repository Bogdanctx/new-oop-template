{
  "recreateWhen": "always",
  "enabledManagers": ["regex"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^\\.github/workflows/[^/]+\\.ya?ml$",
        "^\\.github/actions/.+\\.ya?ml$"
      ],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>\\S+) depName=(?<depName>\\S+) versioning=(?<versioning>\\S+)\\s+uses:\\s*\\S+@(?<currentValue>[^\\s#]+)"
      ],
      "datasourceTemplate": "{{{datasource}}}",
      "depNameTemplate": "{{{depName}}}",
      "extractVersionTemplate": "^v?(?<version>.*)$",
      "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver-coerced{{/if}}",
      "autoReplaceStringTemplate": "# renovate: datasource={{{datasource}}} depName={{{depName}}} versioning={{{versioning}}}\nuses: {{depName}}@v{{{newValue}}}"
    }
  ],
  "branchConcurrentLimit": null,
  "prConcurrentLimit": null,
  "branchHourlyLimit": null,
  "prHourlyLimit": null
}
